generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  DATABASE_URL="file:./dev.db"
}

model Rental {
  id String @id @default(uuid())
  carId String
  userId String
  startDate DateTime
  expectedReturnDate DateTime
  endDate DateTime?
  
  @@map("rentals")
}

